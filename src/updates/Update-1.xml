<?xml version="1.0" encoding="utf-8" ?>
<update>
  <action code="Exec" condition="TableExists" criteria="Circuitscape_OutputFile">
    <item>DROP TABLE Circuitscape_OutputFile</item>
  </action>
  <action code="Exec" condition="TableExists" criteria="ConnCons_REnvironment">
    <item>DROP TABLE ConnCons_REnvironment</item>
  </action>
  <action code="Exec" condition="TableExists" criteria="ConnCons_CPBiodiversityFeatureList">
    <item>DROP TABLE ConnCons_CPBiodiversityFeatureList</item>
  </action> 
  <action code="ChangeNamespace" oldNamespace="ConnCons" newNamespace="stconnect">
  </action> 
  <action code="Exec" condition="TableExists" criteria="corestime_Charts">
    <item>UPDATE corestime_Charts SET Criteria = REPLACE(Criteria, 'ConnCons_', 'stconnect_')</item>
    <item>UPDATE corestime_Charts SET Criteria = REPLACE(Criteria, 'NCEquivalentConnectivityGap', 'stconnect_NCEquivalentConnectivityGap')</item> 
    <item>UPDATE corestime_Charts SET Criteria = REPLACE(Criteria, 'NCEquivalentConnectivityNatal', 'stconnect_NCEquivalentConnectivityNatal')</item>     
  </action>   
  <action code="Exec" condition="TableExists" criteria="corestime_Maps">
    <item>UPDATE corestime_Maps SET Criteria = REPLACE(Criteria, 'ConnCons_', 'stconnect_')</item>
    <item>UPDATE corestime_Maps SET Criteria = REPLACE(Criteria, 'HSHabitatSuitability', 'stconnect_HSHabitatSuitability')</item> 
    <item>UPDATE corestime_Maps SET Criteria = REPLACE(Criteria, 'HSHabitatPatch', 'stconnect_HSHabitatPatch')</item> 
    <item>UPDATE corestime_Maps SET Criteria = REPLACE(Criteria, 'HSResistance', 'stconnect_HSResistance')</item> 
    <item>UPDATE corestime_Maps SET Criteria = REPLACE(Criteria, 'Betweenness', 'stconnect_Betweenness')</item>
    <item>UPDATE corestime_Maps SET Criteria = REPLACE(Criteria, 'CumulativeCurrent', 'stconnect_CumulativeCurrent')</item>
    <item>UPDATE corestime_Maps SET Criteria = REPLACE(Criteria, 'SpatialConservationPriorities', 'stconnect_SpatialConservationPriorities')</item>    
  </action>   
</update>
